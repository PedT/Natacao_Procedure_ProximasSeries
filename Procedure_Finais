CREATE OR REPLACE PROCEDURE proximas_series_finais
	IS
	raia_c NUMBER;
	raia_d NUMBER;
	iterador NUMBER;
	maximo NUMBER;
	BEGIN

	FOR prova_cursor IN (SELECT id_prova FROM PROVA ORDER BY id_prova)
	raia_d := 4;
	raia_c := 5;
	iterador := 4;
	maximo := 0;
	LOOP
		FOR mcursor IN (SELECT nome_partic_sp SERIE_PARTICIPANTE S1 WHERE S1.data_hora_sp.tipo = 'S' AND S1.data_hora_sp.id_prova_sr = prova_cursor.id_prova AND S1.tempo > 0 ORDER BY S1.tempo)
		LOOP
		INSERT INTO SERIE_PARTICIPANTE
		 (data_hora_sp, nome_partic_sp, raia)
		SELECT (data_hora, mcursor.nome_partic_sp, iterador)
		FROM SERIE S2
		WHERE S2.tipo = 'F' AND S2.id_prova_sr = mcursor.data_hora_sp.id_prova_sr 
		
		IF iterador < 5 THEN
			raia_d := raia_d - 1;
			iterador := raia_c;
			
			IF raia_d = 0 THEN
				raia_d := 4;
			END IF;
		ELSE
			raia_c := raia_c + 1;
			iterador := raia_d;
			
			IF raia_c = 9 THEN
				raia_c = 5;
			END IF;
		END IF;
		
		maximo := maximo + 1;
		
		IF maximo = 8 THEN
			EXIT;
		END IF;
		
		END LOOP;
	END LOOP;
	
END
